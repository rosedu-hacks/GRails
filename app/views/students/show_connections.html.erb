<p id="notice"><%= notice %></p>
<h2> Students which enrolled to the exam and live close to you </h2>

<% if !@studens.nil? %>
<table>
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Email</th>
    <th>Facebook account</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @studens.each do |student| %>
  <tr>
    <td><%= student.first_name %></td>
    <td><%= student.last_name %></td>
    <td><%= student.email %></td>
    <td><%= '#'%></td>
    <td><%= link_to 'Show', student %></td>
  </tr>
<% end %>
</table>

<% else %>
<h4> No one enrolled with a Facebook Account </h4>
<% end %>
